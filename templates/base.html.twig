<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('global') }}
        {% endblock %}
    </head>
    <body>
        <div class="container">
            <div class="row">
                {% block body %}{% endblock %}
            </div>
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('global') }}
        {% endblock %}
    </body>
</html>
